{
  "version": 2,
  "name": "medical-chatbot-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://medical-chatbot-backend-wzll.onrender.com"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://medical-chatbot-backend-wzll.onrender.com"
    }
  }
}
